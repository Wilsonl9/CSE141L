0010_01000	// st $r8
1000_00000	// ldi 0
0010_00011	// st $r3
0011_01000	// ld $r8
0010_00000	// st $r0
1000_01111	// ldi 15
0010_00100	// st $r4
1000_00100	// ldi 4
0010_00111	// st $r7
0110_00001	// memld $r1
0010_00101	// st $r5
0011_00101	// ld $r5
0100_00100	// and $r4
0101_00000	// xor $r0
1101_00011	// brz MATCH
0011_00100	// ld $r4
1001_00001	// shl 1
0010_00100	// st $r4
0011_00000	// ld $r0
1001_00001	// shl 1
0010_00000	// st $r0
0011_00111	// ld $r7
0001_01111	// sub $one
0010_00111	// st $r7
1101_00100	// brz NEXT_ELEMENT
1011_00001	// jmp COMPARE
0011_00011	// ld $r3
0000_01111	// add $one
0010_00011	// st $r3
0011_00010	// ld $r2
0001_01111	// sub $one
0010_00010	// st $r2
1101_00101	// brz DONE
0011_00001	// ld $r1
0000_01111	// add $one
0010_00001	// st $r1
1011_00010	// jmp LOAD
0011_00011	// ld $r3
0111_00110	// memst $r6
101100000	// done $r6
